generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ApiSpend {
  id String @id @default(cuid())

  createdAt DateTime @default(now()) @map("created_at")

  document  String
  method    String
  pathname  String?
  url       String?
  userAgent String? @map("user_agent")

  @@map("api_spends")
}
