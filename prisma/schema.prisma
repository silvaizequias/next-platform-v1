generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_MANAGER")
}

model Configuration {
  id              String        @id                 @default(cuid())
  createdAt       DateTime      @default(now())     @map("created_at")
  updatedAt       DateTime      @updatedAt          @map("updated_at")

  @@map("configurations")
}