generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ApiSpend {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  createdAt DateTime @default(now()) @map("created_at")
  document  String
  method    String
  url       String

  @@map("api_spends")
}
