generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ApiSpend {
  id String @id @default(cuid())

  createdAt DateTime @default(now()) @map("created_at")

  organization String
  method       String
  path         String
  origin       String?

  @@map("api_spends")
}
